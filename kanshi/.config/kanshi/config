profile {
  output "Dell Inc. DELL U3219Q 5DHYXV2" {
    enable
    mode 3840x2160
  }
  output "eDP-1" disable
}

profile {
  output "Hewlett Packard LA2405 CN4210079X" {
    enable
    transform 270
  }
  output "eDP-1" enable
}

profile {
  # sway-output would be `position 0 0` but `kanshi` uses old syntax and needs commas
  output "Philips Consumer Electronics Company PHILIPS FTV 0x01010101" position 0,0
}
